{
	"name": "codefend-panel",
	"private": true,
	"version": "25.1.1",
	"description": "Security Platform Panel",
	"type": "module",
	"scripts": {
		"start": "tsc && vite",
		"dev": "vite",
		"lint": "eslint --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
		"format": "prettier --check \"**/*.{js,jsx,ts,tsx}\" --ignore-path .prettierignore .",
		"format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .prettierignore .",
		"checking": "pnpm lint:fix && pnpm format:fix && pnpm types:check",
		"types:check": "tsc --noEmit --pretty",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"spellcheck": " git diff --name-only | npx cspell --file-list stdin"
	},
	"dependencies": {
		"@stripe/react-stripe-js": "3.7.0",
		"@stripe/stripe-js": "7.3.1",
		"@tauri-apps/api": "2.8.0",
		"@tauri-apps/plugin-dialog": "2.4.0",
		"@tauri-apps/plugin-notification": "2.3.1",
		"@tauri-apps/plugin-process": "2.3.0",
		"@tauri-apps/plugin-shell": "2.3.1",
		"@tauri-apps/plugin-updater": "2.9.0",
		"@tauri-apps/plugin-upload": "2.3.1",
		"@types/react-window": "^1.8.8",
		"axios": "^1.11.0",
		"chart.js": "4.4.9",
		"d3": "^7.8.5",
		"framer-motion": "^12.23.3",
		"isomorphic-dompurify": "2.25.0",
		"react": "19.1.0",
		"react-chartjs-2": "5.3.0",
		"react-dom": "19.1.0",
		"react-masonry-css": "1.0.16",
		"react-router": "7.6.2",
		"react-router-dom": "7.6.2",
		"react-toastify": "11.0.5",
		"react-virtualized-auto-sizer": "^1.0.26",
		"react-window": "^1.8.11",
		"swr": "2.3.6",
		"usehooks-ts": "3.1.1",
		"zustand": "5.0.5"
	},
	"devDependencies": {
		"@eslint/compat": "1.3.0",
		"@eslint/eslintrc": "3.3.1",
		"@eslint/js": "10.0.0",
		"@tauri-apps/cli": "2.8.4",
		"@types/d3": "^7.4.3",
		"@types/eslint__js": "9.14.0",
		"@types/node": "22.18.1",
		"@types/react": "19.1.12",
		"@types/react-dom": "19.1.9",
		"@typescript-eslint/eslint-plugin": "8.34.0",
		"@typescript-eslint/parser": "8.34.0",
		"@vitejs/plugin-react-swc": "4.0.1",
		"cross-env": "10.0.0",
		"eslint": "9.26.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-prettier": "5.4.1",
		"eslint-plugin-react-hooks": "6.0.0",
		"eslint-plugin-react-refresh": "0.4.20",
		"eslint-plugin-unicorn": "56.0.1",
		"globals": "16.3.0",
		"immutable": "^5.1.3",
		"lightningcss": "1.30.1",
		"prettier": "3.4.2",
		"sass": "1.86.2",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0",
		"vite": "7.1.4",
		"vite-plugin-compression": "0.5.1",
		"zod-validation-error": "^3.5.3"
	},
	"main": "vite.config.ts",
	"keywords": [],
	"author": "Codefend",
	"resolutions": {
		"minimist": "1.2.6"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/codefen/codefend-user.git"
	},
	"bugs": {
		"url": "https://github.com/codefen/codefend-user/issues"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
