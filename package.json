{
	"name": "codefend-panel",
	"private": true,
	"version": "0.0.1",
	"description": "Security Platform Panel",
	"type": "module",
	"scripts": {
		"start": "tsc && vite",
		"dev": "vite",
		"lint": "eslint --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
		"format": "prettier --check \"**/*.{js,jsx,ts,tsx}\" --ignore-path .prettierignore .",
		"format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\" --ignore-path .prettierignore .",
		"checking": "pnpm lint:fix && pnpm format:fix && pnpm types:check",
		"types:check": "tsc --noEmit --pretty",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "WEBKIT_DISABLE_COMPOSITING_MODE=1 tauri"
	},
	"dependencies": {
		"@stripe/react-stripe-js": "^2.7.0",
		"@stripe/stripe-js": "^3.3.0",
		"@tauri-apps/api": "1.5.3",
		"@tauri-apps/cli": "1.5.11",
		"axios": "1.6.8",
		"chart.js": "4.4.2",
		"d3": "7.9.0",
		"isomorphic-dompurify": "^2.7.0",
		"react": "18.3.1",
		"react-chartjs-2": "5.2.0",
		"react-dom": "18.3.1",
		"react-masonry-css": "1.0.16",
		"react-router": "6.21.2",
		"react-router-dom": "6.21.2",
		"react-toastify": "10.0.5",
		"swr": "^2.2.5",
		"usehooks-ts": "3.1.0",
		"zustand": "4.5.2"
	},
	"devDependencies": {
		"@eslint/compat": "1.2.3",
		"@eslint/eslintrc": "3.2.0",
		"@eslint/js": "9.16.0",
		"@types/d3": "7.4.3",
		"@types/eslint__js": "8.42.3",
		"@types/node": "20.12.7",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"@typescript-eslint/eslint-plugin": "8.15.0",
		"@typescript-eslint/parser": "8.15.0",
		"@vitejs/plugin-react-swc": "3.6.0",
		"eslint": "9.10.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-react-hooks": "5.0.0",
		"eslint-plugin-react-refresh": "0.4.16",
		"eslint-plugin-unicorn": "56.0.1",
		"globals": "15.13.0",
		"lightningcss": "1.24.1",
		"lint-staged": "15.2.2",
		"prettier": "3.2.5",
		"sass": "1.75.0",
		"typescript": "5.4.5",
		"typescript-eslint": "8.17.0",
		"unplugin-lightningcss": "0.2.1",
		"vite": "5.2.8"
	},
	"main": "vite.config.ts",
	"keywords": [],
	"author": "Codefend",
	"resolutions": {
		"minimist": "1.2.6"
	}
}